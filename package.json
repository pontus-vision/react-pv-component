{
  "name": "react-pv-components",
  "main": "dist/index.js",
  "types": "types/types.d.ts",
  "version": "0.1.8",
  "private": false,
  "dependencies": {
    "ag-grid-community": "^22.1.1",
    "ag-grid-react": "^22.1.1",
    "axios": "^0.21.1",
    "i18next": "^19.0.3",
    "js-base64": "^2.5.1",
    "path": "^0.12.7",
    "react": "^16.0.0",
    "react-burger-menu": "^2.6.13",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-resize-aware": "^3.0.0",
    "react-select": "^3.0.8",
    "reflexbox": "^4.0.6",
    "semantic-ui-react": "^0.88.2"
  },
  "scripts": {
    "build:parcel": "parcel build src/index.ts",
    "build:rollup": "rollup -c",
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "publish:npm": "rm -rf build && mkdir build &&  babel src/ -d build --copy-files",
    "prettier": "prettier --print-width 120 --single-quote --trailing-comma all   --write \"src/**\" \"example/*.js\"",
    "clean": "rimraf lib dist",
    "lint": "tslint src/*",
    "build:lib": "tsc",
    "build:umd": "webpack src/index.ts -o dist/react-pv-components.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.ts -o dist/react-pv-components.min.js --config webpack.config.production.js",
    "example": "webpack-dev-server --config webpack.config.example.js",
    "build:example": "webpack --config webpack.config.example.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "check": "npm run lint",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "mocha --require ts-node/register --require @babel/register --require tests/setup.js tests/src/*.spec.js --exit",
    "coverage": "nyc npm run test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^0.13.0 || ^0.14.0 || ^15.0.1 || ^16.0.0",
    "react-dom": "^0.13.0 || ^0.14.0 || ^15.0.1 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.4.4",
    "@rollup/plugin-json": "^4.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/lodash": "^4.14.132",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.7.2",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/sinon": "^7.0.12",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "css-loader": "^3.4.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "6.6.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^3.0.0",
    "jsdom": "^15.1.0",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.17.1",
    "pretty-quick": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-resolve": "0.0.1-predev.1",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "sinon": "7.4.2",
    "style-loader": "^1.1.3",
    "svelte": "^3.17.1",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.8.2"
  }
